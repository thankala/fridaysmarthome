{{#if users}}
{{#each users}}
<tr>
    <td>{{userID}}</td>
    <td>{{username}}</td>
    <td>{{fname}}</td>
    <td>{{lname}}</td>
    <td>{{email}}</td>
    <td>{{registerDate}}</td>
    <td>{{lastLogIn}}</td>
    {{#if lastActivity}}
    <td>{{lastActivity}}</td>
    {{else if lastLogOut}}
    <td>{{lastLogOut}}</td>
    {{else}}
    <td>More than a day ago</td>
    {{/if}}
    <td>{{numOfRooms}}</td>
    <td>{{numOfDevices}}</td>
    <td><a href="/users/delete/?id={{userID}}" class="button" id="delete_button">Delete</a></td>
</tr>
{{/each}}
{{/if}}